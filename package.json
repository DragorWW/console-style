{
  "name": "@dragorww/console-style",
  "version": "1.0.0",
  "homepage": "https://dragorww.github.io/console-style/",
  "description": "âœ¨ Styled console.log() for and browser ðŸŽ¨ ðŸ’…",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DragorWW/console-style.git"
  },
  "bugs": {
    "url": "https://github.com/DragorWW/console-style/issues"
  },
  "main": "dist/console$.js",
  "module": "dist/console$.mjs",
  "browser": "dist/console$.umd.js",
  "types": "dist/console$.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc",
    "build:esm": "vite build",
    "build:site": "npm run build && vite build -c vite.site.config.ts",
    "clean": "rimraf dist dist-site",
    "dev": "vite -c vite.site.config.ts",
    "preview": "vite preview -c vite.site.config.ts",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\"",
    "test": "vitest"
  },
  "keywords": [
    "console",
    "styling",
    "colors",
    "chalk"
  ],
  "author": "DragorWW <dragorww@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.0.0",
    "rimraf": "^5.0.0",
    "prettier": "^3.1.0",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "vite": "^5.0.0",
    "@types/node": "^20.0.0"
  }
}
